<?xml version='1.1' encoding='UTF-8'?>
<jenkins.plugins.nodejs.tools.NodeJSInstallation_-DescriptorImpl plugin="nodejs@1.2.7">
  <installations class="jenkins.plugins.nodejs.tools.NodeJSInstallation-array">
    {% for installation in jenkins_nodejs_installations %}
    <jenkins.plugins.nodejs.tools.NodeJSInstallation>
      <name>{{ installation.name }}</name>
      <properties>
        <hudson.tools.InstallSourceProperty>
          <installers>
            <jenkins.plugins.nodejs.tools.NodeJSInstaller>
              <id>{{ installation.version }}</id>
              <npmPackagesRefreshHours>{{ installation.npm_refresh_hours | default(72) }}</npmPackagesRefreshHours>
              <force32Bit>{{ (installation.force_32bit | default(false)) | ternary('true', 'false') }}</force32Bit>
            </jenkins.plugins.nodejs.tools.NodeJSInstaller>
          </installers>
        </hudson.tools.InstallSourceProperty>
      </properties>
    </jenkins.plugins.nodejs.tools.NodeJSInstallation>
    {% endfor %}
  </installations>
</jenkins.plugins.nodejs.tools.NodeJSInstallation_-DescriptorImpl>