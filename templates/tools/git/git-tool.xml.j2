<?xml version='1.1' encoding='UTF-8'?>
<hudson.plugins.git.GitTool_-DescriptorImpl plugin="git-client@2.7.4">
  {% if jenkins_git_installations | count > 0 %}
  <installations class="hudson.plugins.git.GitTool-array">
    {% for installation in jenkins_git_installations %}
    <hudson.plugins.git.GitTool>
      <name>{{ installation.name }}</name>
      <home>{{ installation.home }}</home>
      <properties/>
    </hudson.plugins.git.GitTool>
    {% endfor %}
  </installations>
  {% else %}
  <installations class="hudson.plugins.git.GitTool-array" />
  {% endif %}
</hudson.plugins.git.GitTool_-DescriptorImpl>